                                                                                                    @if (item.CurrentChapter == false)
                                                                                                    {
                                                                                                        <a href="@Url.Action("MakeCurrentChapter", "Home", new { id = item.Id })"><i class="fa fa-calendar-check-o" style="margin-right: 3px; font-size: 16px"></i> Make Current</a><br />
                                                                                                    }
